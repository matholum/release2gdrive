{
  "name": "release2gdrive",
  "author": "Killian Hale",
  "version": "0.0.1-alpha.1",
  "license": "MIT",
  "bin": "./src/index.js",
  "main": "./src/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.lib.json",
    "clean": "ts-node ./scripts/clean.ts",
    "commit": "./node_modules/cz-customizable/standalone.js",
    "check-commit": "ts-node ./scripts/commit-lint.ts",
    "check-lock-files": "ts-node ./scripts/check-lock-files.ts",
    "copy-package-json": "ts-node ./scripts/copy-package-json.ts",
    "lint": "ts-node ./scripts/lint.ts",
    "postinstall": "husky install",
    "test": "jest"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/jest": "^28.1.6",
    "@types/minimist": "^1.2.2",
    "@types/underscore": "^1.11.4",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.31.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.9.1",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "glob": "^8.0.3",
    "husky": "^8.0.1",
    "import-sort-style-renke": "^6.0.0",
    "jest": "^28.1.3",
    "minimist": "^1.2.6",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-import-sort": "^0.0.7",
    "semver": "^7.3.7",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx, .cjs": {
      "parser": "typescript",
      "style": "renke"
    }
  },
  "packageManager": "yarn@3.1.1",
  "dependencies": {
    "google-auth-library": "^8.0.2",
    "googleapis": "^105.0.0",
    "underscore": "^1.13.4"
  }
}
